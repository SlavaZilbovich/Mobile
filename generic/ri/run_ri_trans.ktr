        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[String dim_fields_list;
String fields_values;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

	long column_order = get(Fields.In, "column_order").getInteger(r);

    if (column_order==1)
    {
		dim_fields_list = "";
		fields_values = "";
    }

	String column_name = get(Fields.In, "column_name").getString(r);
	String default_value = get(Fields.In, "default_value").getString(r);
	String dim_schema = get(Fields.In, "dim_schema").getString(r);
	String dim_table = get(Fields.In, "dim_table").getString(r);
	String dim_key = get(Fields.In, "dim_key").getString(r);
	String fact_schema = get(Fields.In, "fact_schema").getString(r);
	String fact_table = get(Fields.In, "fact_table").getString(r);
	String fact_column = get(Fields.In, "fact_column").getString(r);


	Boolean is_last_dim_row = get(Fields.In, "is_last_dim_row").getBoolean(r);

	dim_fields_list = dim_fields_list + column_name;
	if (!is_last_dim_row) dim_fields_list = dim_fields_list + ",";
	fields_values = fields_values + default_value;
	if (!is_last_dim_row) fields_values = fields_values + ",";

	if (is_last_dim_row) {
	    r = createOutputRow(r, 50000);
		String ri_statement = 
"INSERT INTO " + dim_schema + "." + dim_table + " (" + dim_fields_list + 
") select " + fields_values + " from (select distinct " + fact_column + " as " + dim_key + " from " + fact_schema + "." + fact_table + 
" where " + fact_column + " is not null minus select distinct " + dim_key + " from " + dim_schema + "." + dim_table + ")";
		get(Fields.Out, "ri_statement").setValue(r, ri_statement);
		// Send the row on to the next step.
		putRow(data.outputRowMeta, r);
	}

	return true;
}
]]></class_source>
        </definition>
