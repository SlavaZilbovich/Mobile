    <sql>select r.fact_schema,r.fact_table,r.fact_column,r.dim_schema,r.dim_table,r.dim_key,cast&#x28;a.attname as varchar&#x29; as column_name,a.attnum as column_order, a.attlen as column_length,cast&#x28;ty.typname as varchar&#x29; as column_type,&#xa;case when ty.typname in &#x28;&#x27;timestamp&#x27;,&#x27;varchar&#x27;,&#x27;char&#x27;&#x29; and cast&#x28;a.attname as varchar&#x29;&#x3c;&#x3e;r.dim_key then &#x27;&#x27;&#x27;&#x27; else &#x27;&#x27; end &#x7c;&#x7c;&#xa;case &#xa;	when a.attname&#x3d;&#x27;ri_indicator&#x27; then &#x27;true&#x27;&#xa;	when cast&#x28;a.attname as varchar&#x29;&#x3d;r.dim_key then r.dim_key&#xa;	else nvl&#x28;ricd.default_value,ritd.default_value,&#x27;null&#x27;&#x29; &#xa;end &#x7c;&#x7c; &#xa;case when ty.typname in &#x28;&#x27;timestamp&#x27;,&#x27;varchar&#x27;,&#x27;char&#x27;&#x29; and cast&#x28;a.attname as varchar&#x29;&#x3c;&#x3e;r.dim_key then &#x27;&#x27;&#x27;&#x27; else &#x27;&#x27; end &#xa;as default_value&#xa;, case when lead &#x28;a.attnum&#x29; over&#x28;partition by r.fact_schema, r.fact_table, r.fact_column,r.dim_table order by a.attnum&#x29; is null then true else false end as is_last_dim_row&#xa;from etl.ri_relations r&#xa;join pg_tables t on cast&#x28;t.tablename as varchar&#x29;&#x3d;r.dim_table and cast&#x28;t.schemaname as varchar&#x29;&#x3d;r.dim_schema&#xa;join pg_class c on c.relname&#x3d;t.tablename&#xa;join pg_namespace ns on c.relnamespace&#x3d;ns.oid and ns.nspname&#x3d;t.schemaname&#xa;join pg_attribute a on a.attrelid&#x3d;c.oid&#xa;join pg_type ty on a.atttypid&#x3d;ty.oid&#xa;left join etl.ri_column_defaults ricd on cast&#x28;t.schemaname as varchar&#x29;&#x3d;ricd.table_schema and cast&#x28;t.tablename as varchar&#x29; &#x3d;ricd.table_name and cast&#x28;a.attname as varchar&#x29; &#x3d;ricd.column_name&#xa;left join etl.ri_type_defaults ritd on ritd.column_type&#x3d;cast&#x28;ty.typname as varchar&#x29;&#xa;where r.fact_table&#x3d;&#x27;&#x24;&#x7b;FACT_TABLE&#x7d;&#x27; and r.fact_schema&#x3d;&#x27;&#x24;&#x7b;FACT_SCHEMA&#x7d;&#x27; and a.attnum&#x3e;&#x3d;1&#xa;order by r.fact_schema,r.fact_table,r.fact_column,r.dim_table,a.attnum&#x3b;&#xa;</sql>
